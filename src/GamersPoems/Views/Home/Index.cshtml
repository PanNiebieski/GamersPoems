@model IEnumerable<GamersPoems.Models.Game>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>


    <h1>Hi</h1>
    <table>
        <tbody>
            <tr>
                <td>Id</td>
                <td>Name</td>
            </tr>
        </tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
            </tr>
        }
    </table>
</body>
</html>